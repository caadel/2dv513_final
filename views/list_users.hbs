<nav>
  <a class="home" href="/">Home</a>
</nav>

<div class="list-headings list-item">
  <span class="list-id">ID</span>
  <span class="list-title">Name</span>
  <span class="list-title">Password</span>
  <span class="list-title">Email</span>
</div>
<div class="list-container">
  {{#each data}}
  <div class="list-item">
    <span class="list-id">{{this.id}}</span>
    <span class="list-title">{{this.name}}</span>
    <span class="list-title">{{this.password}}</span>
    <span class="list-title">{{this.email}}</span>
  </div>
  {{else}}
  <p class="empty">No content</p>
  {{/each}}
</div>